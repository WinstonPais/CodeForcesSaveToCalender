{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://codeforces.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Super Forces Save to Calender",
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "manifest_version": 2
}
